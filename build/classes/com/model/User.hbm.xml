<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->
<class name="com.model.User" table="user" catalog="foreigntea">

    <id
        name="username"
        type="java.lang.String"
        column="username"
    >
    
        <generator class="assigned" />
    </id>

    <property
        name="password"
        type="java.lang.String"
        column="password"
        not-null="true"
        length="45"
    />
    <property
        name="identity"
        type="java.lang.String"
        column="identity"
        not-null="true"
        length="45"
    />
    <property
        name="teanum"
        type="java.lang.String"
        column="teanum"
        length="10"
    />

    <!-- Associations -->
  

</class>
</hibernate-mapping>
